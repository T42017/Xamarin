<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage Title="{local:Translate PagesText}"
    xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:local="clr-namespace:Enigma"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:views="clr-namespace:Enigma.Views;assembly=Enigma"
            x:Class="Enigma.NavigationPageTabbed">

    <local:MotionPage Title="{local:Translate MotionPageText}" />
    <views:SettingsPage Title="{local:Translate SettingsPageText}" />
    <views:DiagnosticsPage Title="{local:Translate DiagnosticPageText}" />
    <local:MapPage Title="{local:Translate MapText}" />
</TabbedPage>